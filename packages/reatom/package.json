{
  "name": "reatom",
  "version": "0.0.1",
  "description": "Minimal React state management",
  "author": "linq2js <linqtojs@gmail.com>",
  "repository": {
    "url": "https://github.com/linq2js/reatom"
  },
  "homepage": "https://linq2js.github.io/reatom/",
  "license": "ISC",
  "main": "dist/main.js",
  "types": "dist/tsc/main.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "dist/tsc"
  ],
  "keywords": [
    "redux",
    "oop",
    "model",
    "state",
    "state management",
    "react"
  ],
  "peerDependencies": {
    "react": "^16.8.3 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "scripts": {
    "prepublishOnly": "yarn test && yarn && yarn build",
    "lint": "eslint lib/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest --passWithNoTests",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js && gzip-size ./dist/main.js"
  }
}
